<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
   <Label fx:id="dialog" minHeight="-Infinity" text="Label" wrapText="true" HBox.hgrow="SOMETIMES">
         <font>
            <Font name="Monospaced Bold" size="13.0" />
         </font>
      </Label>
    <StackPane style="-fx-border-color: white; -fx-border-radius: 10px; -fx-background-radius: 10px; -fx-padding: 2px; -fx-background-color: pink;">
         <StackPane style="-fx-border-color: white; -fx-border-width: 2px; -fx-border-radius: 10px; -fx-background-radius: 10px; -fx-padding: 2px; -fx-background-color: white;">
            <children>
               <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" />
            </children>
         </StackPane>
    </StackPane>
  </children>
  <padding>
    <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
  </padding>
</fx:root>
